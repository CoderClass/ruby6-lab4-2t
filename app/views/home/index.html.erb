<h1>ChatRoom</h1>
<% @rooms.each do |room| %>
  <div class="btn-block">
    <%= room.name %>
    <%= link_to "Show", room_path(room.id) %>
  </div>
<% end %>

<%= form_for Room.new, url: {controller: "rooms", action: "create"} do |f| %>
    Name: <%= f.text_field :name %><br />
    <%= f.submit nil, label: "Create" %>
<% end %>
